{
  "name": "sky-scrapper-api",
  "version": "1.0.0",
  "description": "Awesome Scrapper",
  "repository": "https://github.com/SkyZeroZx/Sky-Scrapper-API",
  "author": "SkyZeroZx",
  "private": true,
  "engines": {
    "node": "16.17.0"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"e2e/**/*.ts\"",
    "start": "node dist/main",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage --forceExit",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./e2e/jest-e2e.json",
    "test:e2e:watch": "jest --config ./e2e/jest-e2e.json --watch",
    "test:e2e:cov": "jest --config ./e2e/jest-e2e.json --coverage --forceExit",
    "sonar": "sonar-scanner"
  },
  "dependencies": {
    "@nestjs/axios": "^1.0.0",
    "@nestjs/common": "9.2.1",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "9.2.1",
    "@nestjs/jwt": "^10.0.1",
    "@nestjs/mapped-types": "*",
    "@nestjs/mongoose": "^9.2.1",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "9.2.1",
    "@nestjs/schedule": "^2.1.0",
    "@nestjs/serve-static": "^3.0.0",
    "@nestjs/terminus": "^9.1.4",
    "@simplewebauthn/server": "^7.0.0",
    "@willsoto/nestjs-prometheus": "^4.7.0",
    "bcrypt": "^5.1.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.0",
    "cors": "^2.8.5",
    "cron": "^2.2.0",
    "date-fns": "^2.29.3",
    "date-fns-tz": "^1.3.7",
    "dotenv": "^16.0.3",
    "generate-password": "^1.7.0",
    "helmet": "6.0.1",
    "ioredis": "^5.2.4",
    "mongoose": "^6.8.2",
    "nest-winston": "^1.8.0",
    "nodemailer": "^6.8.0",
    "npm-check-updates": "^16.0.5",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "prom-client": "^14.1.0",
    "puppeteer": "^19.4.1",
    "puppeteer-core": "^19.4.1",
    "puppeteer-extra": "^3.3.4",
    "puppeteer-extra-plugin-stealth": "^2.11.1",
    "puppeteer-page-proxy": "^1.3.0",
    "redis": "^4.3.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "user-agents": "^1.0.1247",
    "uuid": "^9.0.0",
    "web-push": "^3.5.0",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1",
    "winston-logstash": "^1.1.0",
    "winston-transport": "^4.5.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.1.5",
    "@nestjs/schematics": "^9.0.3",
    "@nestjs/testing": "^9.2.1",
    "@simplewebauthn/typescript-types": "^7.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cache-manager": "^4.0.2",
    "@types/cors": "^2.8.13",
    "@types/cron": "^2.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "29.2.4",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.0.0",
    "@types/nodemailer": "^6.4.7",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.35",
    "@types/supertest": "^2.0.12",
    "@types/user-agents": "^1.0.2",
    "@types/uuid": "^9.0.0",
    "@types/web-push": "^3.3.2",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "29.3.1",
    "jest-sonar": "^0.2.14",
    "jest-sonar-reporter": "^2.0.0",
    "node-mocks-http": "^1.12.1",
    "prettier": "^2.3.2",
    "sonar-scanner": "^3.1.0",
    "source-map-support": "^0.5.20",
    "supertest": "^6.3.3",
    "ts-jest": "29.0.3",
    "ts-loader": "^9.4.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.0.0",
    "typescript": "^4.3.5"
  },
  "jestSonar": {
    "reportPath": "coverage",
    "reportFile": "sonar-report.xml",
    "indent": 4
  },
  "jest": {
    "moduleNameMapper": {
      "^@core(.*)$": "<rootDir>/core/$1"
    },
    "testTimeout": 15000,
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\**.(controller|service|guard|decorator|interceptor|strategy).spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "testResultsProcessor": "jest-sonar-reporter",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.(t|j)s",
      "!**/*.dto.(t|j)s",
      "!**/*.entity.(t|j)s",
      "!**/**.mock.spec.(t|j)s",
      "!**/*.module.(t|j)s",
      "!**/*.enum.(t|j)s",
      "!**/config/logger/**",
      "!**/core/interceptor/**",
      "!**/core/interface/**",
      "!**core/helpers/**",
      "!**core/metrics/**",
      "!**main.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
